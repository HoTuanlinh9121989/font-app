{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "core-components": ["src/core-components"],
      "core-components/*": ["src/core-components/*"],
      "api/*": ["src/api/*"],
      "api": ["src/api"],
      "model/*": ["src/model/*"],
      "model": ["src/model"],
      "pages": ["src/pages"],
      "pages/*": ["src/pages/*"],
      "utils": ["src/utils"],
      "utils/*": ["src/utils/*"]
    },
  },
  "include": [
    "src"
  ]
}
